	border: 0px;
}

QTabBar::tab {
	margin: 0px;
    border-top: 0px;
	border-left: 1px solid #BABAB8;
	border-right: 1px solid #BABAB8;
	border-bottom: 1px solid #BABAB8;
	background-color: #D4D4D4;
	width: 200px;
	/*min-width: 100px;*/
	height: 32px;
}

QTabBar::tab:selected {
	border-top:2px solid #66A800;
	border-left: 0px;
	border-right: 0px;
	border-bottom: 0px;
	background-color: #E7E6E5;
}

QTabBar::tab:previous-selected {
	border-left: 1px solid #BABAB8;
}

QTabBar::tab:!selected {
	color: #737373;
}
QTabBar QToolButton::right-arrow {
